{
  "name": "@technote-space/get-diff-action",
  "version": "6.0.2",
  "description": "GitHub actions to get git diff.",
  "keywords": [
    "github",
    "github actions"
  ],
  "homepage": "https://github.com/technote-space/get-diff-action",
  "bugs": {
    "url": "https://github.com/technote-space/get-diff-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/get-diff-action.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "files": [
    "lib",
    "action.yml"
  ],
  "scripts": {
    "build": "tsc && yarn ncc build lib/main.js && rm -rf lib",
    "cover": "jest --coverage",
    "postinstall": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/husky ] || husky install",
    "lint": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --cache",
    "lint:fix": "eslint --fix 'src/**/*.ts' '__tests__/**/*.ts'",
    "prepublishOnly": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --disable",
    "postpublish": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --enable",
    "release": "yarn release-ga --test",
    "test": "yarn lint && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@technote-space/filter-github-action": "^0.5.62",
    "@technote-space/github-action-helper": "^5.2.28",
    "@technote-space/github-action-log-helper": "^0.1.51",
    "multimatch": "^6.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@technote-space/github-action-test-helper": "^0.7.41",
    "@technote-space/release-github-actions-cli": "^1.8.9",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nock": "^13.2.4",
    "pinst": "^3.0.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
